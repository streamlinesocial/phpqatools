{
    "name": "streamlinesocial/phpqatools",
    "type": "metapackage",
    "keywords": ["php", "qa", "tools", "debug"]
    "require": {
        "symfony/symfony": "2.2.x-dev",
        "symfony/finder": "2.2.x-dev",
        "pdepend/pdepend" : "1.1.*",
        "phpmd/phpmd" : "1.4.*",
        "squizlabs/php_codesniffer": "1.*",
        "sebastian/phpcpd": "1.4.*",
        "phpunit/phpunit": "3.7.*",
        "zerkalica/php-code-browser": "dev-master",
        "phploc/phploc": "dev-master",
        "theseer/phpdox": "0.5.*@dev"
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
